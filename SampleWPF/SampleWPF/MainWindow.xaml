<Window x:Class="SampleWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SampleWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="100">
            <Button x:Name="Start" Height="40" Width="60" Margin="10">Starts</Button>
            <Button x:Name="Finish" Height="40" Width="60" Margin="10">Finishes</Button>
            <Button x:Name="Complete" Height="40" Width="60" Margin="10">Completes</Button>
        </StackPanel>
    </Grid>
</Window>
